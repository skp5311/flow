<!doctype html>
<html lang="zh">
<head>
    <meta charset="UTF-8">
    <title>测试</title>
    <script src="https://cdn.bootcss.com/jquery/3.2.1/jquery.min.js"></script>
    <script>
        $(function(){
            $.ajax({
                url : '/demo/json.do',
                success: function(data) {
                    $("#content").html(JSON.stringify(data));
                }
            })
        })
    </script>
</head>
<body>
    <div id="content"></div>
</body>
</html>
